{
	"name": "deep",
	"version": "0.0.1",
	"packageManager": "pnpm@10.14.0",
	"private": true,
	"scripts": {
		"preinstall": "cd macos && pod install && cd ..",
		"dev": "pnpm start & pnpm start:macos",
		"start": "react-native start",
		"start:macos": "react-native run-macos",
		"check": "pnpm compile && pnpm lint && pnpm format",
		"compile": "tsc",
		"lint": "exec biome lint --write",
		"format": "exec biome format --write"
	},
	"dependencies": {
		"react": "19.0.0",
		"react-native": "0.78.3",
		"react-native-macos": "^0.78.6"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@babel/preset-env": "^7.25.3",
		"@babel/runtime": "^7.25.0",
		"@biomejs/biome": "2.2.0",
		"@react-native-community/cli": "15.0.1",
		"@react-native-community/cli-platform-android": "15.0.1",
		"@react-native-community/cli-platform-ios": "15.0.1",
		"@react-native/babel-preset": "0.78.3",
		"@react-native/metro-config": "0.78.3",
		"@react-native/typescript-config": "0.78.3",
		"@types/jest": "^30.0.0",
		"@types/react": "^19.0.0",
		"typescript": "5.0.4"
	},
	"engines": {
		"node": ">=18"
	}
}
